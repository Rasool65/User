{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist",
    "module": "esnext",
    "target": "es5",
    "sourceMap": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "es5"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "rootDir": ".",
    "downlevelIteration": true,
    "jsx": "react-jsx",
    "noImplicitAny": false,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "typeRoots": [
      "./node_modules/@types",
      "../../node_modules/@types"
    ],
    "paths": {
      "@pages/*": [
        "./src/pages/*"
      ],
      "@uikits/*": [
        "./src/uiKits/*"
      ],
      "@layout/*": [
        "./src/layouts/*"
      ],
      "@materialProvider/*": [
        "./src/materialProvider/*"
      ],
      "@redux/*": [
        "./src/redux/*"
      ],
      "@routes/*": [
        "./src/routes/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@config/*": [
        "./src/config/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
    }
  },
  "include": [
    "src/**/*",
    "Help",
  ],
  "exclude": [
    "./node_modules/**/*"
  ]
}